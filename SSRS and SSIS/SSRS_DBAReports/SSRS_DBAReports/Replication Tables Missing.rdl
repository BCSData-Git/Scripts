          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="HasPK">
          <DataField>HasPK</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
