  <DataSources>
    <DataSource Name="DataSource1">
      <DataSourceReference>RR-SQL01_SQLMon</DataSourceReference>
      <rd:SecurityType>None</rd:SecurityType>
      <rd:DataSourceID>ba81705d-8415-4f46-a188-c9e35b9e0d1c</rd:DataSourceID>
    </DataSource>
  </DataSources>
  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>
select sc.name as schema_name, t.name as tablename, Case When dt.TABLE_NAME is not null Then 'No PK Constraint' Else '' End As HasPK
from sys.tables t
  inner join sys.schemas sc on t.schema_id = sc.schema_id
	left join sysarticles s on t.name = s.name and sc.name = s.dest_owner
	left join (select t.table_schema, t.table_name  
				from information_schema.tables t
				  left join information_schema.table_constraints tc on t.table_name = tc.table_name 
						and tc.constraint_type = 'PRIMARY KEY'
				where t.table_type = 'BASE TABLE'
				  and tc.table_name is null) dt on dt.table_schema = sc.name and dt.table_name = t.name
where s.dest_owner is null
  and s.name is null
  and t.name not in ('systranschemas', 'sysschemaarticles', 'MSpeer_topologyrequest', 'sysarticleupdates'
	, 'MSpeer_request', 'MSpeer_conflictdetectionconfigrequest', 'syspublications', 'sysarticlecolumns', 'syssubscriptions'
	, 'MSpub_identity_range', 'MSpeer_conflictdetectionconfigresponse', 'MSpeer_topologyresponse', 'sysreplservers'
	, 'sysarticles', 'MSpeer_response', 'MSpeer_lsns', 'MSpeer_originatorid_history', 'ChangeTrackingCurrentVersion' )
  and sc.name not in ('HangFire')
order by sc.name, t.name</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="schema_name">
          <DataField>schema_name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="tablename">
